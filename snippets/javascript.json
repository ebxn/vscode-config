{
  /* general */
  "Import Default Export": {
    "prefix": "imp",
    "body": "import $1 from '$2'",
  },
  "Import Named Export": {
    "prefix": "imd",
    "body": "import { $1 } from '$2'",
  },
  
  /* vue */
  "Vue Data": {
    "prefix": "vdata",
    "body": [
      "data: () => ({",
      "\t$0",
      "}),"
    ]
  },
  "Vue Component Import": {
    "prefix": "vcmp",
    "body": [
      "import $1 from '@/components/$2.vue'"
    ]
  },
  "Vue Route": {
    "prefix": "vroute",
    "body": [
      "{",
      "\tpath: '/$1',",
      "\tname: '$2',",
      "\tcomponent: () => import(/* webpackChunkName: \"$3\" */ '@/views/$2.vue')",
      "},"
    ]
  }
}
