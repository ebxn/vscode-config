{
  "console.log();":          { "prefix": "con", "body": "console.log($0);" },
  "import x from y":         { "prefix": "imp", "body": "import $0 from \"$1\";" },
  "import { x } from y":     { "prefix": "imd", "body": "import { $0 } from \"$1\";" },
  "Vue: Import Component":   { "prefix": "vc",  "body": "import $1 from \"@/components/$2.vue\";" },
  "Vue: Import Vuex Helper": { "prefix": "vx",  "body": "import { $0 } from \"vuex\";" },

  "Vue: Data": {
    "prefix": "vd",
    "body": [
      "data: function () {",
      "\treturn {",
      "\t\t$0",
      "\t}",
      "},"
    ]
  },
  "Vue: Route": {
    "prefix": "vr",
    "body": [
      "{",
      "\tpath: '/$1',",
      "\tname: '$2',",
      "\tcomponent: () => import(/* webpackChunkName: \"$1\" */ '@/views/$2.vue')",
      "},"
    ]
  },
  "Vue: VueX Boilerplate": {
    "prefix": "vxbp",
    "body": [
      "const state = {}",
      "",
      "const mutations = {}",
      "",
      "const actions = {}",
      "",
      "const getters = {}",
      "",
      "export default {",
      "\tnamespaced: true,",
      "\tstate,",
      "\tactions,",
      "\tmutations,",
      "\tgetters",
      "}"
    ]
  }
}
