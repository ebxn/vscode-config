{
  /* GENERAL */
  "Import Default Export": {
    "prefix": "imp",
    "body": "import $1 from '$2'",
  },
  "Import Named Export": {
    "prefix": "imd",
    "body": "import { $1 } from '$2'",
  },
  "Query Selector": {
    "prefix": "ds",
    "body": [
      "document.querySelector($1)"
    ]
  },
  "Query Selector All": {
    "prefix": "dsq",
    "body": [
      "document.querySelectorAll($1)"
    ]
  },
  "Console Log": {
    "prefix": "con",
    "body": [
      "console.log($1)"
    ]
  }
}
