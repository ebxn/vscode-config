{
  "console.log()": {
    "prefix": "con",
    "body": "console.log($0)"
  },

  "querySelector": {
    "prefix": "ds",
    "body": "document.querySelector($1)"
  },

  "querySelectorAll": {
    "prefix": "dsa",
    "body": "document.querySelectorAll($1)"
  },

  "Vue: Import Component": {
    "prefix": "vc",
    "body": "import $1 from '@/components/$2.vue'"
  },

  "Vue: Import Vuex Helper": {
    "prefix": "vx",
    "body": "import { $0 } from 'vuex'"
  },

  "Vue: Component Data": {
    "prefix": "data",
    "body": [
      "data() {",
      "\treturn {",
      "\t\t$0",
      "\t}",
      "},"
    ]
  },

  "Vue: Define Route": {
    "prefix": "vr",
    "body": [
      "{",
      "\tpath: '/$1',",
      "\tname: '$2',",
      "\tcomponent: () => import(/* webpackChunkName: \"$1\" */ '@/views/$2.vue')",
      "},"
    ]
  },

  "Vue: VueX Boilerplate": {
    "prefix": "vxbp",
    "body": [
      "const state = {}",
      "",
      "const mutations = {}",
      "",
      "const actions = {}",
      "",
      "const getters = {}",
      "",
      "export default {",
      "\tnamespaced: true,",
      "\tstate,",
      "\tactions,",
      "\tmutations,",
      "\tgetters",
      "}"
    ]
  }
}
