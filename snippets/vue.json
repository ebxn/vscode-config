{
  "Vue: SFC": {
    "prefix": "vinit",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script>",
      "export default {",
      "\tname: '$1'",
      "}",
      "</script>"
    ]
  },

  "Vue: SFC (SCSS)": {
    "prefix": "vinits",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script>",
      "export default {",
      "\tname: '$1'",
      "}",
      "</script>",
      "",
      "<style lang=\"scss\" scoped>",
      "\t",
      "</style>"
    ]
  },

  "Vue: <template />": {
    "prefix": "template",
    "body": [
      "<template>",
      "\t$0",
      "</template>"
    ]
  },

  "Vue: <script />": {
    "prefix": "script",
    "body": [
      "<script>",
      "\t$0",
      "</script>"
    ]
  },

  "Vue: <style />": {
    "prefix": "style",
    "body": [
      "<style$1 scoped>",
      "\t$0",
      "</style>"
    ]
  }

  /* "Vue: GraphQL Page Query": {
    "prefix": "pquery",
    "body": [
      "<page-query>",
      "\t$0",
      "</page-query>"
    ]
  },

  "Vue: GraphQL Static Query": {
    "prefix": "squery",
    "body": [
      "<static-query>",
      "\t$0",
      "</static-query>"
    ]
  } */
}
