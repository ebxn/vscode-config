{
  "Init": {
    "prefix": "vinit",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script>",
      "export default {",
      "\tname: '$1'",
      "}",
      "</script>"
    ]
  },
  "Init w/ Styles": {
    "prefix": "vinits",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script>",
      "export default {",
      "\tname: '$1'",
      "}",
      "</script>",
      "",
      "<style lang=\"scss\" scoped>",
      "\t",
      "</style>"
    ]
  },
  "Style Tags": {
    "prefix": "style",
    "body": [
      "<style $1 scoped>",
      "\t$0",
      "</style>"
    ]
  },

  /* gridsome */
  "GraphQL Page Query": {
    "prefix": "pquery",
    "body": [
      "<page-query>",
      "\t$0",
      "</page-query>"
    ]
  },
  "GraphQL Static Query": {
    "prefix": "squery",
    "body": [
      "<static-query>",
      "\t$0",
      "</static-query>"
    ]
  }
}
