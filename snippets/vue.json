{
  /* vue 2 */
  "Vue 2: SFC": {
    "prefix": "vinit2",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script>",
      "export default {",
      "\tname: '$1'",
      "}",
      "</script>"
    ]
  },

  "Vue 2: SFC (SCSS)": {
    "prefix": "vinits2",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script>",
      "export default {",
      "\tname: '$1'",
      "}",
      "</script>",
      "",
      "<style lang=\"scss\" scoped>",
      "",
      "</style>"
    ]
  },

  "Vue: <template />": {
    "prefix": "template",
    "body": [
      "<template>",
      "\t$0",
      "</template>"
    ]
  },

  "Vue: <script />": {
    "prefix": "script",
    "body": [
      "<script>",
      "$0",
      "</script>"
    ]
  },

  "Vue: <script setup />": {
    "prefix": "script:setup",
    "body": [
      "<script setup>",
      "$0",
      "</script>"
    ]
  },

  "Vue: <style />": {
    "prefix": "style",
    "body": [
      "<style$1 scoped>",
      "$0",
      "</style>"
    ]
  },

  /* vue 3 */
  "Vue 3: SFC": {
    "prefix": "vinit",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script setup>",
      "//",
      "</script>"
    ]
  },

  "Vue 3: SFC (SCSS)": {
    "prefix": "vinits",
    "body": [
      "<template>",
      "\t$0",
      "</template>",
      "",
      "<script setup>",
      "import {} from 'vue'",
      "</script>",
      "",
      "<style lang=\"scss\" scoped>",
      "",
      "</style>"
    ]
  }
}
