{
  "vuepress:property": {
    "prefix": "prop",
    "body": [
      "### ${0:name}",
      "",
      "- **Type:** `type`",
      "- **Default:** `type`",
      "",
      "Brief description."
    ]
  },
  "vuepress:method": {
    "prefix": "met",
    "body": [
      "### ${0:name}",
      "",
      "- **Arguments:**",
      "",
      "\t- `{type} name = default`",
      "",
      "- **Usage:**",
      "",
      "\t```js",
      "\t// code snippet",
      "\t```",
      "",
      "\tDescription paragraphs.",
      "",
      "- **See also:** [link](file.md#title)"
    ]
  },
  "vuepress:eslint-rule": {
    "prefix": "esr",
    "body": [
      "## rule-name",
      "",
      "Summary",
      "",
      "- reason 1",
      "- reason 2",
      "",
      ":::danger INCORRECT",
      "",
      "```js",
      "// code snippet(s)",
      "```",
      "",
      ":::",
      "",
      ":::tip CORRECT",
      "",
      "```js",
      "// code snippet(s)",
      "```",
      "",
      ":::",
      "",
      "<!--  -->"
    ]
  }
}
