{
  "console.log()": {
    "prefix": "clog",
    "body": "console.log($0)"
  },

  "React: Class Component": {
    "prefix": "rcinit",
    "body": [
      "import React, { Component } from 'react'",
      "",
      "class ${1:Class} extends Component {",
      "\tstate = {}",
      "",
      "\trender() {",
      "\t\treturn (",
      "\t\t\t$0",
      "\t\t)",
      "\t}",
      "}",
      "",
      "export default ${1:Class}"
    ],
  },

  "React: Class Component (w/ Constructor)": {
    "prefix": "rccinit",
    "body": [
      "import React, { Component } from 'react'",
      "",
      "class ${1:Class} extends Component {",
      "\tconstructor(props) {",
      "\t\tsuper(props)",
      "\t\tthis.state = {}",
      "\t}",
      "",
      "\trender() {",
      "\t\treturn (",
      "\t\t\t$0",
      "\t\t)",
      "\t}",
      "}",
      "",
      "export default ${1:Class}"
    ],
  },

  "React: Functional Component": {
    "prefix": "rinit",
    "body": [
      "import React from 'react'",
      "",
      "const ${1:Name} = (props) => {",
      "\treturn (",
      "\t\t$0",
      "\t)",
      "}",
      "",
      "export default ${1:Name}"
    ],
  },

  "React: Import PropTypes": {
    "prefix": "impt",
    "body": "import PropTypes from 'prop-types'"
  },

  "Native: Import": {
    "prefix": "imrn",
    "body": "import { $0 } from 'react-native'"
  },

  "Native: StyleSheet": {
    "prefix": "styles",
    "body": [
      "const styles = StyleSheet.create({",
      "\t$0",
      "})"
    ]
  }

