      "\tconstructor (props) {",
      "\t\tsuper(props)",
      "\t\tthis.state = {}",
      "\t}",
      "",
      "\trender () {",
      "\t\treturn (",
      "\t\t\t$0",
      "\t\t)",
      "\t}",
      "}",
      "",
      "export default ${1:Class}"
    ],
  },
  "React: Functional Component": {
    "prefix": "rinit",
    "body": [
