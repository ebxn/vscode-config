{
  "console.log();":               { "prefix": "con",  "body": "console.log($0);" },
  "import x from y":              { "prefix": "imp",  "body": "import $0 from \"$1\";" },
  "import { x } from y":          { "prefix": "imd",  "body": "import { $0 } from \"$1\";" },
  "React: Import PropTypes":      { "prefix": "impt", "body": "import PropTypes from \"prop-types\";" },
  "React Native: Import Element": { "prefix": "imrn", "body": "import { $0 } from \"react-native\";" },

  /* React Component Constructors */
  "React: Class Component": {
    "prefix": "rcinit",
    "body": [
      "import React, { Component } from \"react\";",
      "",
      "class ${1:Class} extends Component {",
      "\tstate = {};",
      "",
      "\trender() {",
      "\t\treturn (",
      "\t\t\t$0",
      "\t\t);",
      "\t}",
      "}",
      "",
      "export default ${1:Class};"
    ],
  },
  "React: Class Component (w/ Constructor)": {
    "prefix": "rccinit",
    "body": [
      "import React, { Component } from \"react\";",
      "",
      "class ${1:Class} extends Component {",
      "\tconstructor(props) {",
      "\t\tsuper(props);",
      "\t\tthis.state = {};",
      "\t}",
      "",
      "\trender() {",
      "\t\treturn (",
      "\t\t\t$0",
      "\t\t);",
      "\t}",
      "}",
      "",
      "export default ${1:Class};"
    ],
  },
  "React: Functional Component": {
    "prefix": "rinit",
    "body": [
      "import React from \"react\";",
      "",
      "const ${1:Name} = (props) => {",
      "\treturn (",
      "\t\t$0",
      "\t);",
      "};",
      "",
      "export default ${1:Name};"
    ],
  },


  /* React Native */
  "React Native: Define Stylesheet": {
    "prefix": "rnss",
    "body": [
      "const styles = StyleSheet.create({",
      "\t$0",
      "});"
    ]
  }
}
