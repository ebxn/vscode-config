{
  "console.log()": {
    "prefix": "con",
    "body": [
      "console.log($1)"
    ]
  },
  "import x from 'y'": {
    "prefix": "imp",
    "body": [
      "import $1 from '$2'"
    ]
  },
  "import { x } from 'y'": {
    "prefix": "imd",
    "body": [
      "import { $1 } from '$2'"
    ]
  },
  "this.x = this.x.bind(this)": {
