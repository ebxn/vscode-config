      "<transition",
      "\tenter-active-class=\"\"",
      "\tenter-from-class=\"\"",
      "\tenter-to-class=\"\"",
      "\tleave-active-class=\"\"",
      "\tleave-from-class=\"\"",
      "\tleave-to-class=\"\"",
      ">",
      "\t$0",
      "</transition>"
