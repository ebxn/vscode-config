{
  "console.log()": {
    "prefix": "clog",
    "body": "console.log($1)"
  },

  "querySelector": {
    "prefix": "ds",
    "body": "document.querySelector($1)"
  },

  "querySelectorAll": {
    "prefix": "dsa",
    "body": "document.querySelectorAll($1)"
  },

  "createElement": {
    "prefix": "dce",
    "body": "document.createElement($1)"
  },

  "Vue: Import from Vue": {
    "prefix": "vv",
    "body": "import { $1 } from 'vue'"
  },

  "Vue: Import from VueUse": {
    "prefix": "vu",
    "body": "import { $1 } from '@vueuse/core'"
  },

  "Vue: Import Composable": {
    "prefix": "vc",
    "body": "import $1 from '@/composables/$1'"
  }
}
